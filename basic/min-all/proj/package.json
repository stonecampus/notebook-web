{
  "name": "test",
  "version": "0.0.0",
  "description": "A medium website template for Stome Campus",
  "main": "",
  "scripts": {
    "build": "DEBUG=min-web* npm run clean && npm run build:vendor && npm run build:webpack",
    "build:prod": "NODE_ENV=production npm run build",
    "build:vendor": "npm run clean:vendor && webpack --config webpack.vendor.js",
    "build:webpack": "npm run clean:webpack && webpack",
    "clean": "npm run clean:vendor && npm run clean:webpack",
    "clean:vendor": "rimraf dist/vendor/*",
    "clean:webpack": "rimraf dist/assets/*",
    "dev": "npm run build:webpack && DEBUG=min-web* nodemon --watch src app.js",
    "ana": "webpack --env.ana",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "semistandard src client test",
    "lint:css": "stylelint --config etc/stylelint.config.js client/less/**/*.less",
    "test": "NODE_ENV=test DOT_ENV=test/env.test mocha",
    "testw": "NODE_ENV=test DOT_ENV=test/env.test mocha --watch"
  },
  "license": "MIT",
  "author": "stonecampus <stone@mytraces.net>",
  "semistandard": {
    "env": "mocha"
  },
  "dependencies": {
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-favicon": "^2.0.0",
    "nunjucks": "^3.0.1"
  },
  "devDependencies": {
    "bootstrap": "^3.3.7",
    "browser-sync": "^2.18.8",
    "bs-fullscreen-message": "^1.1.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "css-loader": "^0.28.0",
    "debug": "^3.1.0",
    "expose-loader": "^0.7.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "filesize": "^3.5.6",
    "handlebars": "^4.0.6",
    "jquery": "^3.2.1",
    "jsdom": "^11.5.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "nodemon": "^1.11.0",
    "rimraf": "^2.5.4",
    "scdev": "^0.0.0",
    "semistandard": "^12.0.0",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^18.0.0",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.2"
  }
}
